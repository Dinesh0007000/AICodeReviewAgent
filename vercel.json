{
    "version": 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb",
                "memory": 2048,
                "timeout": 120
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    }
}
