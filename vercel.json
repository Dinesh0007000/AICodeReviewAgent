{
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "bash setup-build.sh && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ]
}
