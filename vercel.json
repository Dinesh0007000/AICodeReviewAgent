{
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "bash setup-build.sh && pip install -r requirements.txt",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/vercel/path0/src"
    }
}
