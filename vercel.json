{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm install eslint"
            }
        },
        {
            "src": "src/app.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "apt-get update && apt-get install -y openjdk-11-jre && pip install -r requirements.txt",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/vercel/path0/src"
    }
}
